{
    "version": 2,
    "builds": [
        {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "." }
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/index.html"
        }
    ],
    "headers": [
        {
        "source": "/(.*)",
        "headers": [
            {"key": "Access-Control-Allow-Origin", "value": "*"}
        ]
        }
    ]
}